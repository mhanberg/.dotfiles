#!/usr/bin/env bash

#                __  _ __      ____      _ __
#   __  ______  / /_(_) /     / __/___ _(_) /
#  / / / / __ \/ __/ / /_____/ /_/ __ `/ / /
# / /_/ / / / / /_/ / /_____/ __/ /_/ / / /
# \__,_/_/ /_/\__/_/_/     /_/  \__,_/_/_/
#

((count = 0))

while "$@"; do
  ((count++))
done

echo "Count: $count"
